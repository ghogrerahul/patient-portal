    "@vitest/coverage-v8": "^1.6.0",
    "jsdom": "^24.1.0",
    "terser": "^5.31.0",
    "vite": "^5.0.8",
    "vitest": "^1.6.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3"
